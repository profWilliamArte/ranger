---
   
    import Layout from '../../layout/Layout.astro'; 
     import { Image } from 'astro:assets';
    import manager from '../../img/manager.jpg';
    import entrenador from '../../img/entrenador.jpg';
    import pel01 from '../../img/categorias/u10/pel01.jpg'
---

<Layout titulo="U10">
    <div class="container">
        <div class="text-center text-black-50 pt-4">
            <h3 class="text-center py-4 bg-white">CATEGORIA U10</h3>
            <div class="bg-light">
                <div class="container text-center">
                    <div class="row justify-content-md-center pt-5">
                      <div class="col-md-4 order-2 order-md-1">
                        
                        <Image src={manager} width={150} alt='manager' class="img-fluid rounded-circle"/>
                        <h2 class="fw-normal">MANAGERS</h2>
                        <p>JEFERSON ANDERSON</p>
                      </div>
                      <div class="col-md-4 order-1 order-md-2">
                        <h5 class="py-3">Comprende a los niños nacidos 2014 todo el año, 2013 de mayo a diciembre, 2015 todo el año, 2016 todo el año</h5>
                      </div>
                      <div class="col-md-4 order-3 order-md-3">
                        <Image src={entrenador} width={150} alt='entrenador' class="img-fluid rounded-circle"/>
                        <h2 class="fw-normal">TECNICO</h2>
                        <p>RAUL FRANCO</p>
                        <p></p>
                      </div>
                    </div>
                </div>




       
             

            
            
                
                
                    <!-- START THE FEATURETTES -->
                
                    <hr class="featurette-divider">
                
